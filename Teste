pgt.Text = "Você realmente deseja\nreiniciar o texto?"
pgt.TextColor3 = Color3.fromRGB(255, 255, 255)
pgt.TextSize = 30
pgt.Font = Enum.Font.FredokaOne
pgt.TextWrapped = true

local tweenInfo = TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
local tween = game:GetService("TweenService"):Create(izi, tweenInfo, {Size = UDim2.new(0, 300, 0, 200)})
tween:Play()
end)

fonte.Name = "Fonte"
