nao.MouseButton1Down:Connect(function()
izi.Position = UDim2.new(9999, 0, 9999, 0)
main.Position = UDim2.new(0.020, 0, 0.1, 0)
pgt.Position = UDim2.new(9999, 0, 9999, 0)
end)

local corner2 = Instance.new("UICorner")
