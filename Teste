anotados.TextYAlignment = Enum.TextYAlignment.Top
anotados.Font = Enum.Font.FredokaOne
anotados.TextSize = 24
anotados.BorderSizePixel = 0

stroke2.Name = "Stroke2"
stroke2.Parent = freimi
